{
  "compilerOptions": {
    "isolatedModules": true,
    "paths": {
      "@assets/*": ["src/assets/*"],
      "@config/*": ["src/config/*"],
      "@commons/*": ["src/commons/*"],
      "@configs/*": ["src/configs/*"],
      "@helpers/*": ["src/helpers/*"],
      "@redux/*": ["src/redux/*"],
      "@libraries/*": ["src/libraries/*"],
      "@modules/*": ["src/modules/*"],
      "@constants/*": ["src/constants/*"],
      "@services/*": ["src/services/*"],
      "@utils/*": ["src/utils/*"],
      "@hoc/*": ["src/hoc/*"],
      "@/*": ["src/*"]
    }
  }
}
